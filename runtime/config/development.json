{
  "port": 8360,
  "host": "127.0.0.1",
  "workers": 0,
  "startServerTimeout": 3000,
  "reloadSignal": "SIGUSR2",
  "processKillTimeout": 10000,
  "enableAgent": false,
  "jsonpCallbackField": "callback",
  "jsonContentType": "application/json",
  "errnoField": "errno",
  "errmsgField": "errmsg",
  "defaultErrno": 1000,
  "validateDefaultErrno": 1001,
  "cache": {
    "type": "file",
    "file": {
      "timeout": 86400000,
      "cachePath": "E:\\github\\nav-dashboad\\runtime\\cache",
      "pathDepth": 1,
      "gcInterval": 86400000
    }
  },
  "model": {
    "type": "sqlite",
    "sqlite": {
      "path": "E:\\github\\nav-dashboad\\runtime\\sqlite",
      "database": "data",
      "connectionLimit": 1,
      "prefix": ""
    }
  },
  "session": {
    "type": "file",
    "file": {
      "cookie": {
        "name": "thinkjs"
      },
      "sessionPath": "E:\\github\\nav-dashboad\\runtime\\session"
    }
  },
  "view": {
    "type": "nunjucks",
    "nunjucks": {
      "viewPath": "E:\\github\\nav-dashboad\\view",
      "sep": "_",
      "extname": ".html"
    }
  }
}